<root>
    <description>
        Get a list all clustering algorithms and their properties
    </description>
    <rt>
        XML    
    </rt>
    <URL>
        commu?clnt=name|pass&#38;com=getalgorithms
    </URL>
    <parameter>
        <name>
            clnt
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The Client credentials
        </paramdesc>
        <example>
            clnt=name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            com
        </name>
        <status>
            required
        </status>
        <paramdesc>
            Command to get clustering algorithms
        </paramdesc>
        <example>
            com=getalgorithms
        </example>
    </parameter> 
    <ExURL>
        http://localhost:1111/commu?clnt=name|pass&#38;com=getalgorithms
    </ExURL>
    <ExResponse>
&lt;result&gt;
    &lt;row&gt;
        &lt;algorithm_name&gt;
        betw
        &lt;/algorithm_name&gt;
        &lt;algorithm_value&gt;
        edgesToRemove-&gt;default:5
        &lt;/algorithm_value&gt;
    &lt;/row&gt;
&lt;/result&gt;
    </ExResponse>
</root>
