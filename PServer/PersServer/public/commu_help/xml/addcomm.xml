<root>
    <description>
        Add custom community on PServer
    </description>
    <rt>
        XML
    </rt>
    <URL>
        commu?clnt=&lt;name|pass&gt;&#38;com=addcomm&#38;name=testcommunity&#38;users=[“user1”,”user2”]
    </URL>
    <parameter>
        <name>
            clnt
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The client credentials
        </paramdesc>
        <example>
            clnt=&lt;name|pass&gt;
        </example>
    </parameter> 
    <parameter>
        <name>
            com
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The add community command
        </paramdesc>
        <example>
            com=addcomm
        </example>
    </parameter> 
    <parameter>
        <name>
            name
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The community name
        </paramdesc>
        <example>
            name=testcommunity
        </example>
    </parameter> 
    <parameter>
        <name>
            users
        </name>
        <status>
            required
        </status>
        <paramdesc>
            List with users who belongs
        </paramdesc>
        <example>
            users=[“user1”,”user2”]
        </example>
    </parameter> 
    <ExURL>
        http://localhost:1111/1.0/community/lastfm|experiment123/cluster/algorithms.xml
    </ExURL>
    <ExResponse>
&lt;result&gt;
    &lt;row&gt;
        &lt;algorithm_name&gt;
        betw
        &lt;/algorithm_name&gt;
        &lt;algorithm_value&gt;
        edgesToRemove-&gt;default:5
        &lt;/algorithm_value&gt;
    &lt;/row&gt;
&lt;/result&gt;
    </ExResponse>
</root>
      
