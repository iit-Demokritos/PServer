<root>
    <description>
        Get profile for the given community name
    </description>
    <rt>
        XML/JSON
    </rt>
    <URL>
        1.0/community/:ClientCredentials/community.xml
    </URL>
    <URL>
        1.0/community/:ClientCredentials/community.json
    </URL>
    <parameter>
        <name>
            :ClientCredentials
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The Client credentials
        </paramdesc>
        <example>
            name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            name
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the community name
        </paramdesc>
        <example>
            name= algorithm.assocType.name
        </example>
    </parameter> 
    <parameter>
        <name>
            ftrpattern
        </name>
        <status>
            optional
        </status>
        <paramdesc>
            Profile feature filter. Returns the hole profile if no ftrpattern exists

        </paramdesc>
        <example>
            ftrpattern=txt.*
        </example>
    </parameter> 
    <ExURL>
    
    </ExURL>
    <ExResponse>
    
    </ExResponse>
</root>
