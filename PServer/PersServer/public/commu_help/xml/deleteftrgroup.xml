<root>
    <description>
Delete feature groups with given group pattern name. If pattern name 
is empty then will delete all user communities from PServer
    </description>
    <rt>
    XML
    </rt>
    <URL>
    commu?clnt=&lt;name|pass&gt;&#38;com=deleteftrgroup&#38;pattern=
    </URL>
    <parameter>
        <name>
        clnt
        </name>
        <status>
        required
        </status>
        <paramdesc>
        The Client credentials
        </paramdesc>
        <example>
        clnt=&lt;name|pass&gt;
        </example>
    </parameter> 
    <parameter>
        <name>
        com
        </name>
        <status>
        required
        </status>
        <paramdesc>
        Command to delete feature groups
        </paramdesc>
        <example>
        com=deleteftrgroup
        </example>
    </parameter> 
    <parameter>
        <name>
        pattern
        </name>
        <status>
        optional
        </status>
        <paramdesc>
        The delete pattern name
        </paramdesc>
        <example>
        pattern= algorithm.assocType.name
        </example>
    </parameter> 
    <ExURL>
    
    </ExURL>
    <ExResponse>
    
    </ExResponse>
</root>
