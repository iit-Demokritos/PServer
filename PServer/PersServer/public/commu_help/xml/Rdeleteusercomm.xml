<root>
    <description>
        Delete communities with given community pattern name. If pattern name 
        is empty then will delete all user communities from PServer
    </description>
    <rt>
        XML/JSON
    </rt>
    <URL>
        1.0/community/:ClientCredentials/delete/users.xml
    </URL>
    <URL>
        1.0/community/:ClientCredentials/delete/users.json
    </URL>
    <parameter>
        <name>
            :ClientCredentials
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The Client credentials
        </paramdesc>
        <example>
            name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            pattern
        </name>
        <status>
            optional
        </status>
        <paramdesc>
            The delete pattern name
        </paramdesc>
        <example>
            pattern= algorithm.assocType.name
        </example>
    </parameter> 
    <ExURL>
    
    </ExURL>
    <ExResponse>
    
    </ExResponse>
</root>
