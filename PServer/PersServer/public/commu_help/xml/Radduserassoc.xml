<root>
    <description>
        Add association weight between two users in PServer for specific
        association type 
    </description>
    <rt>
        XML/JSON
    </rt>
    <URL>
        1.0/community/:ClientCredentials/association/custom/users.xml
    </URL>
    <URL>
        1.0/community/:ClientCredentials/association/custom/users.json
    </URL>
    <parameter>
        <name>
            :ClientCredentials
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The Client credentials
        </paramdesc>
        <example>
            name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            user1
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the first user
        </paramdesc>
        <example>
            user1=test1
        </example>
    </parameter> 
    <parameter>
        <name>
            user2
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the second user
        </paramdesc>
        <example>
            user2=test2
        </example>
    </parameter> 
    <parameter>
        <name>
            weight
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the association weight between two users
        </paramdesc>
        <example>
            weight= 0.9
        </example>
    </parameter> 
    <parameter>
        <name>
            association
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the association type
        </paramdesc>
        <example>
            association=5
        </example>
    </parameter> 
    <ExURL>
    
    </ExURL>
    <ExResponse>
    
    </ExResponse>
</root>
