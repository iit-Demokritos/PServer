<root>
    <description>
        Get list of users who consists the given community name
    </description>
    <rt>
        XML
    </rt>
    <URL>
        commu?clnt=name|pass&#38;com=getcommusers&#38;name=type_algorithm_assocType_name&#38;usrpattern=tes*
    </URL>
    <parameter>
        <name>
            clnt
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The Client credentials
        </paramdesc>
        <example>
            clnt=name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            com
        </name>
        <status>
            required
        </status>
        <paramdesc>
            Command to get community users
        </paramdesc>
        <example>
            com=getcommusers
        </example>
    </parameter> 
    <parameter>
        <name>
            name
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the community name
        </paramdesc>
        <example>
            name=com_metis_cos_2
        </example>
    </parameter> 
    <parameter>
        <name>
            usrpattern
        </name>
        <status>
            optional
        </status>
        <paramdesc>
            Community user filter. Returns all if no usrpattern exists
        </paramdesc>
        <example>
            usrpattern=tes*
        </example>
    </parameter> 
    <ExURL>
    
    </ExURL>
    <ExResponse>
    
    </ExResponse>
</root>
