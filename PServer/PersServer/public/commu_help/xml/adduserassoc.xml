<root>
    <description>
        Add association weight between two users in PServer for specific
        association type 
    </description>
    <rt>
        XML
    </rt>
    <URL>
        commu?clnt=name|pass&amp;com=adduserassoc&amp;user1=test1&amp;user2=test2&amp;weight=0.9&amp;association=cos
    </URL>
    <parameter>
        <name>
            clnt
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The Client credentials
        </paramdesc>
        <example>
            clnt=name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            com
        </name>
        <status>
            required
        </status>
        <paramdesc>
            Add user association command
        </paramdesc>
        <example>
            com=adduserassoc
        </example>
    </parameter> 
    <parameter>
        <name>
            user1
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the first user
        </paramdesc>
        <example>
            user1=test1
        </example>
    </parameter> 
    <parameter>
        <name>
            user2
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the second user
        </paramdesc>
        <example>
            user2=test2
        </example>
    </parameter> 
    <parameter>
        <name>
            weight
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the association weight between two users
        </paramdesc>
        <example>
            weight=0.9
        </example>
    </parameter> 
    <parameter>
        <name>
            association
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the association type
        </paramdesc>
        <example>
            association=5
        </example>
    </parameter> 
    <ExURL>
    
    </ExURL>
    <ExResponse>
    
    </ExResponse>
</root>
