<root>
    <description>
        Get list of features which consists the given feature group name
    </description>
    <rt>
        XML    
    </rt>
    <URL>
        commu?clnt=name|pass&#38;com=getftrgroupfeatures&#38;name=type_algorithm_assocType_name&#38;ftrpattern=tes*
    </URL>
    <parameter>
        <name>
            clnt
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The Client credentials
        </paramdesc>
        <example>
            clnt=name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            com
        </name>
        <status>
            required
        </status>
        <paramdesc>
            Command to get feature group features 
        </paramdesc>
        <example>
            com=getftrgroupfeatures
        </example>
    </parameter> 
    <parameter>
        <name>
            name
        </name>
        <status>
            required
        </status>
        <paramdesc>
            the feature group name
        </paramdesc>
        <example>
            name=type_algorithm_assocType_name
        </example>
    </parameter> 
    <parameter>
        <name>
            ftrpattern
        </name>
        <status>
            optional
        </status>
        <paramdesc>
            Feature group features filter. Returns all if no ftrpattern exists
        </paramdesc>
        <example>
            ftrpattern=tes*
        </example>
    </parameter> 
    <ExURL>
    
    </ExURL>
    <ExResponse>
    
    </ExResponse>
</root>
