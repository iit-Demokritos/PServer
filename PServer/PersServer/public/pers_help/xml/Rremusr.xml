<root>
    <description>
        Removes all Users from user list.
        If no list removes all users.
    </description>
    <rt>
        XML/JSON
    </rt>
    <URL>
        1.0/personal/:ClientCredentials/Delete_Users.xml
    </URL>
    <URL>
        1.0/personal/:ClientCredentials/Delete_Users.json
    </URL>
    <parameter>
        <name>
            :ClientCredentials
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The client credentials
        </paramdesc>
        <example>
            name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            users
        </name>
        <status>
            optional
        </status>
        <paramdesc>
            List with usernames tha i want to delete.
            If no list then remmove all the users.
        </paramdesc>
        <example>
            users={"john","kostas"}
        </example>
    </parameter> 
    <ExURL>
       
    </ExURL>
    <ExResponse>

    </ExResponse>
</root>