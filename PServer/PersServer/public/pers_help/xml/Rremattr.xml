<root>
    <description>
       Delete attributes from PServer
    </description>
    <rt>
        XML/JSON
    </rt>
    <URL>
        1.0/personal/:ClientCredentials/Delete_Attributes.xml
    </URL>
    <URL>
        1.0/personal/:ClientCredentials/Delete_Attributes.json
    </URL>
    <parameter>
        <name>
            :ClientCredentials
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The client credentials
        </paramdesc>
        <example>
            name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            attributes
        </name>
        <status>
            optional
        </status>
        <paramdesc>
            List with attribute names for delete.
            If no list seted then dell all attributes.
        </paramdesc>
        <example>
            attributes={"attr1","attr2"}
        </example>
    </parameter> 
    <ExURL>
       
    </ExURL>
    <ExResponse>

    </ExResponse>
</root>