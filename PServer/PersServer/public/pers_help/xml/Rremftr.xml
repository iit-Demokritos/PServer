<root>
    <description>
        Removes all records with features matching the
        feature pattern(s). If no set pattern alla features wiil be deleted.
    </description>
    <rt>
        XML/JSON
    </rt>
    <URL>
        1.0/personal/:ClientCredentials/Delete_Features.xml
    </URL>
    <URL>
       1.0/personal/:ClientCredentials/Delete_Features.json
    </URL>
    <parameter>
        <name>
            :ClientCredentials
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The client credentials
        </paramdesc>
        <example>
            name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            features
        </name>
        <status>
            required
        </status>
        <paramdesc>
            List with feature patterns
        </paramdesc>
        <example>
            features={"ftr1","ftr2"}
        </example>
    </parameter> 
    <ExURL>
       
    </ExURL>
    <ExResponse>

    </ExResponse>
</root>