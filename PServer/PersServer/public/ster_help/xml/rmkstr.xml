<root>
    <description>
        Removes and reinserts the sterotype specified removing all current
        users and features and adding again all users matching the stereotypes
        rule.
    </description>
    <rt>
        XML
    </rt>
    <URL>
        ster?clnt=name|pass&amp;com=rmkstr&amp;str=str
    </URL>
    <parameter>
        <name>
            clnt
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The client credentials
        </paramdesc>
        <example>
            clnt=name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            com
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The remake stereotype command
        </paramdesc>
        <example>
            com=rmkstr
        </example>
    </parameter> 
    <parameter>
        <name>
            str
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The stereotype name
        </paramdesc>
        <example>
            str=test.1
        </example>
    </parameter> 
    <ExURL>
        ster?clnt=name|pass&amp;com=rmkstr&amp;str=test.1
    </ExURL>
    <ExResponse>
	  &lt;result&gt;
	    &lt;row&gt;
	      &lt;num_of_rows&gt;1&lt;/num_of_rows&gt;
	    &lt;/row&gt;
	  &lt;/result&gt;
    </ExResponse>
</root>