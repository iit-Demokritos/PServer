<root>
    <description>
        Removes all features from the stereotype and then adds and sets
        all features as needed based on the stereotypes current users
        and their degrees.
    </description>
    <rt>
        XML
    </rt>
    <URL>
        ster?clnt=name|pass&amp;com=updftr&amp;str=strname
    </URL>
    <parameter>
        <name>
            clnt
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The client credentials
        </paramdesc>
        <example>
            clnt=name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            com
        </name>
        <status>
            required
        </status>
        <paramdesc>
            Update stereotype features command
        </paramdesc>
        <example>
            com=updftr
        </example>
    </parameter> 
    <parameter>
        <name>
           str
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The stereotype name
        </paramdesc>
        <example>
           str=testStrs.1
        </example>
    </parameter> 
    <ExURL>
        ster?clnt=name|pass&amp;com=updftr&amp;str=testStrs.1
    </ExURL>
    <ExResponse>
	  &lt;result&gt;
	    &lt;row&gt;
	      &lt;num_of_rows&gt;1&lt;/num_of_rows&gt;
	    &lt;/row&gt;
	  &lt;/result&gt;
    </ExResponse>
</root>