<root>
    <description>
       Removes all stereotypes matching str_pattern
    </description>
    <rt>
        XML
    </rt>
    <URL>
       ster?clnt=name|pass&amp;com=remstr&amp;str=str_pattern
    </URL>
    <parameter>
        <name>
            clnt
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The client credentials
        </paramdesc>
        <example>
            clnt=name|pass
        </example>
    </parameter> 
    <parameter>
        <name>
            com
        </name>
        <status>
            required
        </status>
        <paramdesc>
            Remove stereotype command
        </paramdesc>
        <example>
            com=remstr
        </example>
    </parameter> 
    <parameter>
        <name>
            str
        </name>
        <status>
            required
        </status>
        <paramdesc>
            The stereotype pattern
        </paramdesc>
        <example>
            str=test.*
        </example>
    </parameter> 
    <ExURL>
        ster?clnt=name|pass&amp;com=remstr&amp;str=test.*
    </ExURL>
    <ExResponse>
	  &lt;result&gt;
	    &lt;row&gt;
	      &lt;num_of_rows&gt;1&lt;/num_of_rows&gt;
	    &lt;/row&gt;
	  &lt;/result&gt;
    </ExResponse>
</root>